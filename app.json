{
  "name": "CallsMusic Plus",
  "description": "A CallsMusic Project! Edited By Itz-Fork!",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/Itz-fork/Callsmusic-Plus",
  "stack": "container",
  "env": {
    "API_ID": {
      "description": "Your API_ID from my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Your API_HASH from my.telegram.org",
      "required": true
    },
    "SESSION_NAME": {
      "description": "Your Pyrogram String Session. Read wiki.",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Your Bot Token From @BotFather",
      "required": true
    },
    "SUDO_USERS": {
      "description": "ID's of Sudo Users. They admins everywhere (separate each id by space)",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max Time Limite For One Song.",
      "required": true,
      "value": "180"
    },
    "USER_ACCNAME": {
      "description": "Streamer Account Username without '@' !",
      "required": true,
      "value": ""
    }
  }
}
